class Button:
  def paint(self):
    pass

class WinButton(Button):
  def paint(self):
    print("Windows Button")

class MacButton(Button):
  def paint(self):
    print("Mac Button")

class Checkbox:
  def paint(self):
    pass

class WinCheckbox(Checkbox):
  def paint(self):
    print("Windows Checkbox")

class MacCheckbox(Checkbox):
  def paint(self):
    print("Mac Checkbox")

class GUIFactory:
  def create_button(self):
    pass

  def create_checkbox(self):
    pass

class WinFactory(GUIFactory):
  def create_button(self):
    return WinButton()

  def create_checkbox(self):
    return WinCheckbox()

class MacFactory(GUIFactory):
  def create_button(self):
    return MacButton()

  def create_checkbox(self):
    return MacCheckbox()
